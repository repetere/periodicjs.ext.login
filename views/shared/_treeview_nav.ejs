<% var menu_logout,
  navmenu={
    "tree": []
  },
  StylieTreeview = locals.stylietreeview;

  if(user){
    navmenu.tree.push({
      "tree-item": "file",
      "tree-item-label": "Log Out",
      "tree-item-id": "login.logout",
      "tree-item-link": "/"+adminPostRoute+"/logout",
      "tree-item-attributes": {
        "class": "async-admin-ajax-link",
        "title": "Log out of your account"
      }
    });
  }
  else {
    navmenu.tree.push({
      "tree-item": "file",
      "tree-item-label": "Log In",
      "tree-item-id": "login.login",
      "tree-item-link": "/"+adminPostRoute+"/login",
      "tree-item-attributes": {
        "class": "async-admin-ajax-link",
        "title": "Log in to your account"
      }
    });
    navmenu.tree.push({
      "tree-item": "file",
      "tree-item-label": "Register",
      "tree-item-id": "login.register",
      "tree-item-link": "/"+adminPostRoute+"/user/new",
      "tree-item-attributes": {
        "class": "async-admin-ajax-link",
        "title": "Register for a new account"
      }
    });
    navmenu.tree.push({
      "tree-item": "file",
      "tree-item-label": "Forgot password",
      "tree-item-id": "login.forgot",
      "tree-item-link": "/"+adminPostRoute+"/forgot",
      "tree-item-attributes": {
        "class": "async-admin-ajax-link",
        "title": "Recover lost password"
      }
    });
  }


  navTreeview = new locals.stylietreeview({
    tree:navmenu
  });

  navTreeViewHTML = navTreeview.getTreeHTML();

  %>
  <%- navTreeViewHTML %>
<script type="text/javascript">
  <%- locals.viewhelper.passObjToClient(navmenu,"periodic_navmenu") %>
</script>